a=[[" "for i in range(10) ]for i in range (10) ]
for i in range(10):
    for j in range(10):
        if(j==0 or j==9):
            a[i][j]="w"
        if(i+j==9 and i>4 ):
            a[i][j]="w"
        if(i==jas and j>4):
            a[i][j]="w"
for i in range (10):
    for j in range (10):
        print(a[i][j],end=" ")
    print()  
